<div class="row">
<%= simple_form_for(@booking) do |f| %>
<div class="form-group">
<%= f.input :date_start %>
</div>
<div class="form-group">
<%= f.input :date_end %>
</div>
<%= f.input :photo %>
<%= f.input :photo_cache, as: :hidden %>

<%= f.submit class:'btn btn-lg btn-success' %>

<% end %>
</div>
